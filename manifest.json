{
  "manifest_version": 3,
  "name": "Colour Palette Extension",
  "description": "Page to generate a random colour Palette",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "colourPage.html"
  },

  "icons": {
    "16": "PaletteWiz.png",
    "48": "PaletteWiz.png",
    "128": "PaletteWiz.png"
  },
  "browser_action": {
    "default_icon": "PaletteWiz.png"
  }
}
